{
    "version": 2,
    "builds": [
        {
            "src": "Server.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/socket.io/(.*)",
            "dest": "Server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}